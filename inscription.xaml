<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aptruc.inscription"
             Title="inscription">
    <ContentPage.Content>
        <StackLayout Padding="10">
            <Entry x:Name="EntryEmail" Placeholder="Email" />
            <Entry x:Name="EntryNom" Placeholder="Nom" />
            <Entry x:Name="EntryPrenom" Placeholder="Prénom" />
            <Entry x:Name="EntryMdp" Placeholder="Mot de passe" IsPassword="True" />
            <Entry x:Name="EntryTel" Placeholder="Téléphone" Keyboard="Telephone" />
            <Entry x:Name="EntryDateNaissance" Placeholder="date de naissance" />
            <Button Text="Inscrire" Clicked="TentativeInscription" />
            <Button Text="Se connecter" Clicked="Onconnecterclick" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>